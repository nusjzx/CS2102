<h1>New project</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@project) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= f.label :title %>
      <%= f.title_field :title, class: 'form-control' %>

      <%= f.label :descrition %>
      <%= f.descrition_field :descrition, class: 'form-control' %>

      <%= f.label :start_date %>
      <%= f.start_date_field :start_date, class: 'form-control' %>

      <%= f.label :duration%>
      <%= f.duration_field :duration, class: 'form-control' %>
      <%= f.label :total_amt%>
      <%= f.total_amt :total_amt, class: 'form-control' %>

      <%= f.submit "Create", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>